output/*

# But keep the directories themselves
!data/.gitkeep
!output/.gitkeep
!processed/.gitkeep

# Environment variables
.env
.env.local
.env.*.local

# JSON files
*.json

# Python
__pycache__/
*.py[cod]
*$py.class

# Distribution / packaging
dist/
build/
*.egg-info/

# Environment
.venv
env/
venv/
ENV/
env.bak/
venv.bak/

# Data directories (if you don't want to track the data)
data/convert/
data/output/

# macOS specific files
.DS_Store

# IDE specific files
.idea/
.vscode/
*.swp

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class